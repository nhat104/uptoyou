{
  "kind": "collectionType",
  "collectionName": "hits",
  "info": {
    "singularName": "hit",
    "pluralName": "hits",
    "displayName": "HIT",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "jobGuide": {
      "type": "text",
      "required": true
    },
    "reward": {
      "type": "integer",
      "min": 100,
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "available",
        "onworking",
        "submitted",
        "accepted",
        "rejected",
        "expired"
      ],
      "default": "available"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "hits"
    },
    "batch": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::batch.batch",
      "inversedBy": "hits"
    },
    "image": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::image.image",
      "inversedBy": "hit"
    }
  }
}
